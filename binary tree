#include "stdlib.h"
#include "stdio.h"

struct tree_node{
    int value;
    struct tree_node *left;
    struct tree_node *right;
};

void add_node(struct tree_node *new_node){
    int i = 1;
}

void treeprint(struct tree_node *node){
    if (node != NULL) {
        treeprint(node->left);
        printf("%d ", node->value);
        treeprint(node->right);
    }
}

int main(){
    struct tree_node test;
    struct tree_node test_l;
    struct tree_node test_r;
    struct tree_node test_l_l;
    struct tree_node test_l_r;

    test.value = 6;
    test.left = &test_l;
    test.right = &test_r;

    test_l.value = 4;
    test_l.left = &test_l_l;
    test_l.right = &test_l_r;

    test_r.value = 5;
    test_r.left = NULL;
    test_r.right = NULL;

    test_l_l.value = 3;
    test_l_l.left = NULL;
    test_l_l.right = NULL;

    test_l_r.value = 9;
    test_l_r.left = NULL;
    test_l_r.right = NULL;

    treeprint(&test);
}


